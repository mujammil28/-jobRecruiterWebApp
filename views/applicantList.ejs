<h1>Applicants for Job ID <%= applicants.jobId %></h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Contact</th>
      <th>Resume</th>
    </tr>
  </thead>
  <tbody>
    <% applicants.forEach(applicant => { %>
      <tr>
        <td><%= applicant.name %></td>
        <td><%= applicant.email %></td>
        <td><%= applicant.contact %></td>
        <td><a href="<%= applicant.resumePath %>" target="_blank">View Resume</a></td>
      </tr>
    <% }) %>
  </tbody>
</table>
